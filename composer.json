{
    "name": "interaction-design-foundation/laravel-batch-mailer",
    "type": "library",
    "require": {
        "php": "^8.1|^8.2",
        "mailgun/mailgun-php": "^3.5",
        "interaction-design-foundation/laravel-postmark-api": "^v0.1.0",
        "ext-fileinfo": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.10|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "InteractionDesignFoundation\\BatchMailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InteractionDesignFoundation\\BatchMailer\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "mateusjunges",
            "email": "mateus@junges.dev"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:InteractionDesignFoundation/laravel-postmark-api.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "InteractionDesignFoundation\\BatchMailer\\Providers\\BatchMailerServiceProvider"
            ]
        }
    },
  "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
